{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "semicolon": [true, "never"],
        "object-literal-sort-keys": false,
        "no-console": false,
        "space-before-function-paren": [true, {"anonymous": "always", "named": "never", "asyncArrow": "always"}]
    },
    "rulesDirectory": []
}